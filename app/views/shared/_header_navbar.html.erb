<header role="banner">
  <%= render :partial => 'shared/this_is_beta' %>
  <div class="l-branding__pul">
    <div class="container">
      <%= link_to root_path, class: "logo__pul col-xs-12 col-sm-6" do %>
        <%= image_tag("library_full_logo.svg", class: "logo-svg") %>
        <%= image_tag("library_full_logo.png", class: "logo-no-svg") %>
      <% end %>
      <div class="pull-right menu--level-1 col-xs-12 col-sm-6">
        <ul>
          <li><a href="http://library.princeton.edu/help/contact-us">Contact Us</a></li>
          <%= render :partial=>'/account' %>
        </ul>
      </div>
    </div>
  </div>
  <div class="home-secondary-navbar navbar <% if index_layout? %>home<% end %>">
    <div class="container" role="navigation">
      <div class="row logo">
        <div class="col-xs-12 col-sm-2">
          <div class="logo__app">NEWNAME</div>
          <%# link_to root_path, class: "logo__app" do %>
            <%# image_tag 'geo-logo.png', {:alt => 'Digital Maps and Geographic Data', :style => 'height: 40px;', :class => 'pulmap-brand'} %>
          <%# end %>
        </div>
        <%= render :partial=>'catalog/search_form_home2' %>
      </div>
    </div>
  </div>
</header>
